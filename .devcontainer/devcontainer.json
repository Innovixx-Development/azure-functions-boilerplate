{
	"name": "azure-functions-boilerplate",
	"dockerComposeFile": "docker-compose.yml",
	"service": "server",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	"forwardPorts": [
		7071,
		10000,
		10001,
		10002
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-azuretools.vscode-azurefunctions",
				"dbaeumer.vscode-eslint"
			]
		}
	},
	"remoteUser": "vscode"
}